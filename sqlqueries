SELECT TAG, RECORD_MATCHING_STRING
FROM (
    SELECT DISTINCT TAG, RECORD_MATCHING_STRING
    FROM your_table_name
    WHERE ORIGIN IN ('METI', 'BSP') -- Replace with your list of ORIGIN values
    ORDER BY DBMS_RANDOM.VALUE
)
WHERE ROWNUM = 1;

SELECT TAG, RECORD_MATCHING_STRING
FROM (
    SELECT DISTINCT TAG, RECORD_MATCHING_STRING
    FROM DATA2
    WHERE ORIGIN IN ('METI', 'BSP')  -- Replace with your list of ORIGIN values
    AND LENGTH(TAG) > 4
    AND REGEXP_LIKE(TAG, '^[A-Za-z]')
    ORDER BY DBMS_RANDOM.VALUE
)
WHERE ROWNUM <= 10;
